{
  "region": {
    "prefix": "@region",
    "body": "<!--#region ${1:chapter}-->\n${2:}\n<!--#endregion ${1:chapter}-->\n",
    "description": "Region",
    "scope": "markup.list.unnumbered.wikitext"
  },
  "redirect": {
    "prefix": "@redirect",
    "body": "#REDIRECT [[${1:Pagename}]]",
    "description": "Redirection",
    "scope": ""
  },
  "table": {
    "prefix": "@table",
    "body": "{| ${1:class=\"wikitable\"}\n|+Title\n!\n!\n!\n!\n|-\n|\n|\n|\n|\n|-\n|\n|\n|\n|\n|-\n|\n|\n|\n|\n|}\n",
    "description": "Table",
    "scope": ""
  },
  "codeBlock": {
    "prefix": "@codeblock",
    "body": "<syntaxhighlight lang=\"${1:c#}\" line=\"${2:1}\" start=\"${3:1}\">\n\n</syntaxhighlight>\n",
    "description": "Code Block",
    "scope": ""
  },
  "comments": {
    "prefix": "@comments",
    "body": "<!--${1:comments}-->",
    "description": "Comments",
    "scope": ""
  },
  "todo": {
    "prefix": "@todo",
    "body": "<!--TODO:${1:Task}-->",
    "description": "Todo Task",
    "scope": ""
  },
  "title": {
    "prefix": "@title",
    "body": "== ${1:title} ==\n",
    "description": "Title<2>",
    "scope": ""
  },
  "subtitle1": {
    "prefix": "@subt1",
    "body": "=== ${1:title} ===\n",
    "description": "Subtitle1<3>",
    "scope": ""
  },
  "subtitle2": {
    "prefix": "@subt2",
    "body": "==== ${1:title} ====\n",
    "description": "Subtitle2<4>",
    "scope": ""
  },
  "subtitle3": {
    "prefix": "@subt3",
    "body": "===== ${1:title} =====\n",
    "description": "Subtitle3<5>",
    "scope": ""
  },
  "subtitle4": {
    "prefix": "@subt4",
    "body": "====== ${1:title} ======\n",
    "description": "Subtitle4<6>",
    "scope": ""
  },
  "pageTitle": {
    "prefix": "@pagetitle",
    "body": "= ${1:title} =\n",
    "description": "PageTitle<1>",
    "scope": ""
  },
  "pageInfo": {
    "prefix": "@pageinfo",
    "body": "<%-- [PAGE_INFO]\nComment=#${1:}#\nPageTitle=#${2:}#\nPageID=#${3:}#\nRevisionID=#${4:}#\nContentModel=#${5:}#\nContentFormat=#${6:}#\n[END_PAGE_INFO] --%>\n",
    "description": "Page Info Struct",
    "scope": ""
  },
  "refName": {
    "prefix": "@refname",
    "body": "<ref name=\"${1:name}\"/>",
    "description": "Reference tag with name",
    "scope": ""
  },
  "refDefine": {
    "prefix": "@refdefine",
    "body": "<ref name=\"${1:name}\" >${2:definition}</ref>",
    "description": "Reference tag with name and definition",
    "scope": ""
  }
}
